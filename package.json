{
  "name": "ng-gridview",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Bob",
  "scripts": {
    "rimraf": "rimraf",
    "typings": "typings",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "prebuild": "npm run clean:dist",
    "build": "tsc --declaration && npm run build:dist && rm src/gridview.js && rm src/gridview.d.ts && rm src/gridview.js.map",
    "build:dist": "webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "start": "npm run server:dev",
    "postinstall": "npm run typings -- install"
  },
  "dependencies": {
  },
  "devDependencies": {
    "angular": "^1.5.3",
    "awesome-typescript-loader": "^0.16.2",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.6.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "^1.8.10",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
